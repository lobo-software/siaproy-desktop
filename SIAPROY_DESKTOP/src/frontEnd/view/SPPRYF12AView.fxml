<AnchorPane id="AnchorPane" prefHeight="175.0" prefWidth="310.0" styleClass="root" stylesheets="@SPPRYF12AStyle.css" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="frontEnd.controller.SPPRYF12AController">
   <children>
      <VBox alignment="CENTER" layoutX="58.0" layoutY="34.0" prefHeight="175.0" prefWidth="310.0" stylesheets="@SPPRYF12AStyle.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ImageView fx:id="imgLogo" fitHeight="97.0" fitWidth="222.0" pickOnBounds="true" preserveRatio="true">
               <VBox.margin>
                  <Insets bottom="10.0" top="5.0" />
               </VBox.margin></ImageView>
            <HBox alignment="CENTER" spacing="2.0">
               <children>
                  <Label fx:id="lblContraActual" alignment="CENTER_RIGHT" contentDisplay="CENTER" prefHeight="20.0" prefWidth="88.0" text="Usuario: ">
